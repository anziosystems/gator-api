{
  "name": "gator-api",
  "version": "1.0.0",
  "description": "API Service for Gator",
  "main": "./dist/app.js",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "node ./dist/app.js",
    "build:prod": "npm run prestart && npm prune --production",
    "test": "mocha -r ts-node/register Test/*.ts",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "lint": "eslint --ext=ts . ",
    "lint:fix": "eslint --ext=ts .  --fix",
    "posttest": "npm run lint"
  },
  "author": "Rafat Sarosh",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/platform-browser": "^7.2.13",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.4",
    "@types/passport": "^1.0.0",
    "@types/passport-github": "^1.1.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "connect": "^3.6.6",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "jsonwebtoken": "^8.5.1",
    "lint": "^1.1.2",
    "lodash": "^4.17.15",
    "mocha": "^6.1.4",
    "mssql": "^5.0.5",
    "node": "10.14.0",
    "node-cache": "^4.2.0",
    "passport": "^0.4.1",
    "passport-atlassian-oauth2": "^1.0.1",
    "passport-bitbucket": "^2.0.0",
    "passport-bitbucket-oauth2": "^0.1.2",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-openidconnect": "0.0.2",
    "prettier": "^1.16.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rxjs": "^6.4.0",
    "typescript": "^3.4.3",
    "web-request": "^1.0.7",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2"
  }
}